<%= form_tag "/events/#{@event.id}/checkback.js", :id => 'checkback', remote: true do %>
  <%= text_field_tag "uin", nil, :label => false, :placeholder => 'UIN' %>
  <%= text_field_tag "card", @card, :label => false, :placeholder => 'Card Number' %>
  <br></br>
         <%= submit_tag 'Check in' %>
<% end %>