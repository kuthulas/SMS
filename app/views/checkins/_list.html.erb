<%= page_entries_info @checkins, :model => 'checkin' =%>
 <p id="notice"><%= notice %></p>
<table>
    <thead>
      <tr>
        <th>Event</th>
        <th>Term</th>
        <th>Date</th>
        <th>Location</th>
        <th>Time</th>
        <th>Student First Name</th>
        <th>Last Name</th>
        <th>Email</th>
        <th>UIN</th>
        <th>Check-in Time</th>
        <th colspan = 2>Manage</th>
      </tr>
    </thead>

    <tbody>
      <% @checkins.each do |checkin| %>
        <tr>
          <td><%= checkin.event.name %></td>
          <td><%= checkin.event.term %></td>
          <td><%= checkin.event.date %></td>
          <td><%= checkin.event.location %></td>
          <td><%= checkin.event.time %></td>
          <td><%= checkin.student.fname %></td>
          <td><%= checkin.student.lname %></td>
          <td><%= checkin.student.email %></td>
          <td><%= checkin.student.uin %></td>
          <td><%= l checkin.created_at, format: "%I:%M %p" %></td>
          <% if admin_signed_in? %>
          <td><%= link_to 'Edit', edit_checkin_path(checkin) %></td>
          <td><%= link_to 'Destroy', checkin, method: :delete, data: { confirm: 'Are you sure?' } %></td>
          <% end %>
        </tr>
      <% end %>
    </tbody>
  </table>