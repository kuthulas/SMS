<%= simple_form_for(@checkin) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
  	<%= f.input :event_id, label: false, placeholder: 'Event ID' %>
    <%= f.input :student_id, label: false, placeholder: 'Student ID' %>
    <%= f.input :created_at, label: false, placeholder: 'Created At' %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
