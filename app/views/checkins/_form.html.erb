<%= simple_form_for(@checkin) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
  	<%= f.input :event_id %>
    <%= f.input :student_id %>
    <%= f.input :user_id %>
    <%= f.input :created_at %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
